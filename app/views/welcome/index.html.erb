<input type="text" name="zip-search" id="zip-search" /> <button id="zip-search">Submit</button>
<br/>
<div id="centerThis">
  <ul id="displayData" class="collapsible" data-collapsible="accordion"></ul>
</div>
<br><br>
<div id="userSignUp">
  <br><br>
  <%= form_for @user, method: 'post', url: {action: "create"} do |f| %>
    <% if @user.errors.any? %>
      <div>
        <h2>Form is invalid</h2>
        <ul>
          <% for message in @user.errors.full_messages %>
          <li><%= message %></li>
          <% end %>
        </ul>
      </div>
    <% end %>
    <%= label(:user, :name) %>
    <%= f.text_field :name %>
    <%= label(:user, :email) %>
    <%= f.text_field :email %>
    <%= label(:user, :username) %>
    <%= f.text_field :username %>
    <%= label(:user, :password) %>
    <%= f.password_field :password %>
    <%= label(:user, :password_confirmation) %>
    <%= f.password_field :password_confirmation %>
    <br><br>
    <%= f.submit "Sign up" %>
  <% end %>
</div>
